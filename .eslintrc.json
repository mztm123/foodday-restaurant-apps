{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle":"off",
        "no-console" :"off",
        "no-restricted-globals" : "off",
        "no-undef" : "off",
        "linebreak-style" : "off",
        "no-trailing-spaces" : "off",
        "eol-last" : "off",
        "no-shadow" :"off"
    }
}
